{
  "version": 2,
  "builds": [
    {
      "src": "streamlit_app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/streamlit_app.py"
    }
  ],
  "build": {
    "env": {
      "PYTHON_VERSION": "3.9",
      "NODE_VERSION": "18.x",
      "PIP_ONLY_BINARY": ":all:",
      "PIP_IGNORE_INSTALLED": "1"
    }
  },
  "env": {
    "STREAMLIT_SERVER_HEADLESS": "true",
    "STREAMLIT_SERVER_PORT": "8080",
    "STREAMLIT_SERVER_ADDRESS": "0.0.0.0"
  }
}
